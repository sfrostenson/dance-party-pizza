{
  "name": "gulp-starter-backbone",
  "version": "1.0.0",
  "description": "a starter gulp + browserify build system for projects",
  "main": "gulpfile.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sfrostenson/gulp-starter-backbone.git"
  },
  "keywords": [
    "gulp",
    "browserify",
    "npm"
  ],
  "author": "Sarah E. Frostenson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sfrostenson/gulp-starter-backbone/issues"
  },
  "homepage": "https://github.com/sfrostenson/gulp-starter-backbone",
  "browser": {
    "jquery": "./node_modules/jquery/dist/jquery.min.js",
    "bootstrap": "./node_modules/bootstrap/dist/js/bootstrap.min.js"
  },
  "browserify-shim": {
    "jquery": "$",
    "bootstrap": {
      "depends": [
        "jquery"
      ]
    }
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "devDependencies": {
    "browser-sync": "^2.5.3",
    "browserify": "^9.0.4",
    "browserify-shim": "^3.8.3",
    "gulp": "^3.8.11",
    "gulp-autoprefixer": "^2.3.0",
    "gulp-changed": "^1.2.1",
    "gulp-filesize": "0.0.6",
    "gulp-imagemin": "^2.3.0",
    "gulp-minify-css": "^1.1.1",
    "gulp-notify": "^2.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^1.3.3",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.4",
    "jstify": "^0.9.0",
    "require-dir": "^0.1.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.1.0"
  },
  "dependencies": {
    "backbone": "^1.2.0",
    "bootstrap": "^3.3.4",
    "d3": "^3.5.5",
    "jquery": "^2.1.3",
    "lodash": "^3.6.0",
    "topojson": "^1.6.18"
  }
}
